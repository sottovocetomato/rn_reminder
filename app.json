{
  "expo": {
    "name": "Daily Goals",
    "slug": "rn_notify_me",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/dailyGoalsCover.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/dailyGoalsCoverSplash.png",
      "resizeMode": "contain",
      "backgroundColor": "#543b69"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/dailyGoalsCover.png",
        "backgroundColor": "#543b69"
      },
      "package": "com.adrstr.rn_notify_me"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "1369d391-1785-42bf-ba69-925a1c2f5e48"
      }
    }
  }
}
